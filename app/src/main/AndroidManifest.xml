        </activity>
        <activity android:name=".farmermenu" />
        <activity android:name=".vegetable" />
        <activity android:name=".vendorrequest" />
        <activity android:name=".vendorhome" />
        <activity android:name=".farmerrequest" />
        <activity android:name=".vegetable2" />
